{
	"data": [
		"_int db \"%i\", 0x0a, 0x00",
		"_float db \"%f\", 0xA, 0",
    "__dummy_float dq 0.0",
    "_float_in db \"%lf\", 0",
    "_int_in db \"%i\", 0"
	],
	"code": [
		"func_syscall_print_int:",
		"\tpush ebp",
		"\tmov ebp, esp",
		"\tmov eax, [ebp + 8]",
		"\tpush eax",
		"\tpush _int",
		"\tcall printf",
		"\tmov esp, ebp",
		"\tpop ebp",
		"\tret",

		"func_syscall_print_float:",
		"\tpush ebp",
		"\tmov ebp, esp",
		"\tfld dword [ebp + 8]",
		"\tsub esp, 8",
		"\tfstp qword [esp]",
		"\tpush _float",
		"\tcall printf",
		"\tmov esp, ebp",
		"\tpop ebp",
		"\tret",
    
		"syscall_scan_int:",
		"\tpush ebp",
		"\tmov ebp, esp",
	    "\tsub esp, 4",
	    "\tpush esp ",
		"\tpush _int_in",
		"\tcall scanf",
	    "\tmov eax, [ebp - 4]",
		"\tmov esp, ebp",
		"\tpop ebp",
		"\tret",
    
		"func_syscall_scan_float:",
		"\tpush ebp",
		"\tmov ebp, esp",
		"\tpush __dummy_float",
		"\tpush _float_in",
		"\tcall scanf",
		"\tfld qword [__dummy_float]",
		"\tmov esp, ebp",
		"\tpop ebp",
		"\tret"    
	],
	"externs": [
		"printf",
		"scanf"
	]
}