import IO;

class Main {
    int num = 0;

    Main() {
        num += 1;
    }

    void increase(int n) {
        for (int i = 0; i < n; i++) {
            num++;
        }
    }

    void main() {
        Main cc = new Main();
        int a = 10;
        cc.increase(a);

        IO io = new IO();
        io.print_int(a);
    }
}